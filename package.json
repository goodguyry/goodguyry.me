{
  "name": "goodguyry.me",
  "version": "1.0.0",
  "description": "A website",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@infinity-interactive/eleventy-plugin-injector": "0.0.9",
    "autoprefixer": "^9.8.6",
    "concurrently": "^4.1.0",
    "cssnano": "^4.1.10",
    "html-entities": "^1.2.1",
    "postcss": "^7.0.35",
    "postcss-modules": "^1.4.1",
    "postcss-tidy-columns": "^0.4.0",
    "postcss-units": "^1.2.1",
    "read-yaml": "^1.1.0",
    "sass": "^1.29.0",
    "stylelint": "^9.10.1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/goodguyry/goodguyry.me"
  },
  "scripts": {
    "webpack:watch": "webpack --config _client/config/webpack.develop.config.js --watch",
    "webpack:prod": "webpack --config _client/config/webpack.config.js",
    "dev": "concurrently 'npm run webpack:watch' 'npm run 11ty:watch'",
    "build": "npm run webpack:prod && npm run 11ty:prod",
    "11ty:dev": "ELEVENTY_ENV=development npx @11ty/eleventy",
    "11ty:prod": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "11ty:serve": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "11ty:watch": "ELEVENTY_ENV=development npx @11ty/eleventy --watch"
  },
  "author": "Ryan Domingue",
  "license": "GPL-3.0",
  "homepage": "https://github.com/goodguyry/goodguyry.me",
  "dependencies": {
    "fg-loadcss": "^2.0.1",
    "fontfaceobserver": "^1.7.3"
  }
}
