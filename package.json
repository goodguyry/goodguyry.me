{
  "name": "goodguyry.me",
  "version": "1.0.0",
  "description": "A website",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "fg-loadcss": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/goodguyry/goodguyry.me"
  },
  "scripts": {
    "webpack:watch": "webpack --config _client/config/webpack.develop.config.js --watch",
    "webpack:prod": "webpack --config _client/config/webpack.config.js",
    "jekyll:watch": "jekyll build --config _config.yaml --watch --drafts",
    "jekyll:dev": "jekyll build --config _config.yaml --drafts",
    "jekyll:build": "JEKYLL_ENV=production jekyll build --config _config.yaml,_config-prod.yaml",
    "dev": "concurrently 'npm run webpack:watch' 'npm run 11ty:watch'",
    "build": "npm run webpack:prod && npm run jekyll:build",
    "11ty:dev": "ELEVENTY_ENV=development npx @11ty/eleventy",
    "11ty:prod": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "11ty:serve": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "11ty:watch": "ELEVENTY_ENV=development npx @11ty/eleventy --watch"
  },
  "author": "Ryan Domingue",
  "license": "GPL-3.0",
  "homepage": "https://github.com/goodguyry/goodguyry.me",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "fontfaceobserver": "^1.7.3",
    "html-entities": "^1.2.1",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.6",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "postcss-tidy-columns": "^0.4.0",
    "read-yaml": "^1.1.0",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-livereload-plugin": "^2.1.1",
    "webpack-stats-plugin": "^0.2.1",
    "write-file-webpack-plugin": "^4.5.0"
  }
}
