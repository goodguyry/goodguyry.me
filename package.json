{
  "name": "goodguyry.me",
  "version": "1.0.0",
  "description": "A website",
  "devDependencies": {
    "cssnano": "^3.5.2",
    "fg-loadcss": "^1.2.0",
    "fontfaceobserver": "^1.7.1",
    "penthouse": "^0.9.1",
    "uglify-js": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/goodguyry/goodguyry.me"
  },
  "scripts": {
    "uglify": "node _build-scripts/uglify.js",
    "critical": "node _build-scripts/critical.js",
    "nano": "node _build-scripts/nano.js",
    "build": "npm run uglify && JEKYLL_ENV=production jekyll build --config _config.yaml,_config-prod.yaml",
    "webpack": "webpack --config _client/config/webpack.config.js --mode production",
    "dev": "concurrently 'npm run webpack:dev' 'npm run jekyll:dev'",
    "webpack:dev": "webpack --config _client/config/webpack.config.js --mode development --watch",
    "jekyll:dev": "jekyll build --config _config.yaml --watch"
  },
  "author": "Ryan Domingue",
  "license": "GPL-3.0",
  "homepage": "https://github.com/goodguyry/goodguyry.me",
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.6",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "read-yaml": "^1.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-livereload-plugin": "^2.1.1",
    "webpack-stats-plugin": "^0.2.1",
    "write-file-webpack-plugin": "^4.5.0"
  }
